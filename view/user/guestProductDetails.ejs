<script>
  document.getElementById('wishlist-link').addEventListener('click', async function (event) {
    event.preventDefault(); // Prevent the default link action

    // Get product ID from URL
    const productId = "<%= product._id %>";
    const url = `/addToWhishlist?id=${productId}`;

    try {
      // Send AJAX request to backend
      const response = await fetch(url, { method: 'GET' });
      const data = await response.json();

      if (data.success) {
        // Display success message with two buttons
        Swal.fire({
          title: 'Success!',
          text: data.message, // Success message from backend
          icon: 'success',
          showCancelButton: true,
          confirmButtonText: 'Go to Wishlist',
          cancelButtonText: 'OK',
          reverseButtons: true,
        
          position: 'center',
       
          timerProgressBar: true,
        }).then((result) => {
          if (result.isConfirmed) {
            // Redirect to wishlist page if "Go to Wishlist" is clicked
            window.location.href = '/whishlist'; // Replace with your wishlist URL
          }
        });

        // Optionally, change heart color to indicate product is in wishlist
        const heartIcon = document.querySelector('.fa-heart');
        heartIcon.style.color = 'rgb(255, 0, 0)'; // Change to red if added
      } else {
        // Display message if product already exists in wishlist
        Swal.fire({
          title: 'Already in Wishlist',
          text: data.message, // Message when already in wishlist
          icon: 'info',
          showCancelButton: true,
          confirmButtonText: 'Go to Wishlist',
          cancelButtonText: 'OK',
          reverseButtons: true,
         
          position: 'center',
         
          timerProgressBar: true,
        }).then((result) => {
          if (result.isConfirmed) {
            // Redirect to wishlist page if "Go to Wishlist" is clicked
            window.location.href = '/whishlist'; // Replace with your wishlist URL
          }
        });
      }
    } catch (error) {
      console.log(error);
      // Handle errors if needed
    }
  });
</script>




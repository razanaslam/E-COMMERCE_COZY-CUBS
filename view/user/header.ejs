  /* General styles for the navigation menu */
  .main-menu ul li {
    display: inline-block;
    margin: 0 10px;
    position: relative;
  }
  
  .main-menu ul li a {
    color: #333; 
    text-decoration: none;
    padding-bottom: 5px;
    transition: all 0.3s ease; 
  }
  
  /* Hover effect with a custom underline */
  .main-menu ul li a:hover {
    color: #000000; 
    border-bottom: 2px solid #000000; 
  }
  
  /* Optional: Add more spacing for the hover underline effect */
  .main-menu ul li a:hover::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 2px;
    background-color: #000000;
    transform: scaleX(1);
    transition: transform 0.3s ease;
  }
  
  /* Reset styles for no underline initially */
  .main-menu ul li a::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 2px;
    background-color: transparent; 
    transform: scaleX(0); 
    transition: transform 0.3s ease, background-color 0.3s ease;
  }
  </style>
  

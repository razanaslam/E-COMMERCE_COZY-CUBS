        Swal.fire({
          title: "Are you sure?",
          text: "You will be logged out!",
          icon: "warning",
          showCancelButton: true,
          confirmButtonText: "Yes, logout!",
          cancelButtonText: "Cancel",
          confirmButtonColor: "black",
          cancelButtonColor: "black",
          background: "#f8f9fa",
          iconColor: "#dc3545",
        }).then((result) => {
          if (result.isConfirmed) {
            window.location.href = "/logout";
          }
        });
      }
    </script>


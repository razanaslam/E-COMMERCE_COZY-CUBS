    <style>
      /* General table styling */
      table {
        width: 100%;
        border-collapse: collapse; /* Ensures no gaps between cells */
      }

      th,
      td {
        padding: 10px; /* Adds padding to cells */
        text-align: left; /* Aligns text to the left */
      }

      /* Specific styles for buttons */
      button {
        margin: 0 5px; /* Adds horizontal margin to buttons */
        padding: 5px 10px; /* Adds padding for buttons */
        border: none; /* Removes default button border */
        background-color: #007bff; /* Button background color */
        color: white; /* Button text color */
        cursor: pointer; /* Pointer cursor on hover */
        border-radius: 4px; /* Rounded corners */
      }

      button:hover {
        background-color: #0056b3; /* Darker background on hover */
      }

      /* To create a consistent distance */
      .product-img img {
        max-width: 100%; /* Ensures images fit within their container */
        height: auto; /* Maintains aspect ratio */
      }

      .product-name,
      .product-price {
        padding: 0 10px; /* Adds horizontal padding to product name and price */
      }

      /* Add spacing for quantity display */
      .quantity-container {
        display: flex; /* Uses flexbox for better alignment */
        align-items: center; /* Centers items vertically */
      }

      .qty-label {
        margin: 0 10px; /* Adds margin to the quantity label */
      }

      /* Highlight overlaps with bold */
      .overlap {
        font-weight: bold; /* Makes overlapping text bold */
        color: red; /* Change color to red for visibility */
      }
    </style>

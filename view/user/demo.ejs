      <div class="cart-main-area pt-90 pb-100">
        <div class="container">
          <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-12">
              <form action="#">
                <div class="row">
                  <div class="col-lg-8">
                    <div
                      class="table-content table-responsive cart-table-content"
                    >
                      <!-- <p><%#cartEmpty %></p> -->
                      <table>
                        <thead>
                          <tr>
                            <th scope="col">Image</th>
                            <th scope="col">Title</th>
                            <th scope="col">Price</th>
                            <th scope="col">Quantity</th>
                            <th scope="col">Total</th>
                            <th scope="col"></th>
                          </tr>
                        </thead>

                        <tbody>
                          <% let subtotal = 0; cartItems.forEach((item) => {%>
                          <% subtotal+=item.qty * item.price%>
                          <tr>
                            <td class="product-img">
                              <a href="#">
                                <img
                                  style="width: 70%"
                                  src="/uploads/<%= item.product.image_url[0] %>"
                                  alt="<%= item.product.product_title %> image"
                                />
                              </a>
                            </td>
                            <td class="product-name">
                              <a href="#"><%= item.product.product_title %></a>
                            </td>
                            <td class="product-price">
                              <span class="amount"
                                ><%= item.product.discountedPrice %></span
                              >
                            </td>
                            <td>
                              <div class="quantity-container">
                                <button
                                  type="button"
                                  style="
                                    background-color: #fc4f4f;
                                    color: white;
                                  "
                                  onclick="updateQty('<%= item.product._id %>', 'decrement', event)"
                                >
                                  -
                                </button>
                                <span
                                  class="qty-label"
                                  id="qty-<%= item.product._id %>"
                                >
                                  <%= item.qty %>
                                </span>
                                <button
                                  type="button"
                                  style="
                                    background-color: #fc4f4f;
                                    color: white;
                                  "
                                  onclick="updateQty('<%= item.product._id %>', 'increment', event)"
                                >
                                  +
                                </button>
                              </div>
                            </td>
                            <td id="total-<%= item.product._id %>">
                              <%= item.price * item.qty %>
                            </td>
                            <td class="action" data-title="Remove">
                              <button
                                class="text-muted"
                                onclick="removeFromCart('<%= item.product._id %>')"
                                style="
                                  background: none;
                                  border: none;
                                  cursor: pointer;
                                "
                                aria-label="Remove <%= item.product.product_title %> from cart"
                              >
                                X
                              </button>
                            </td>
                          </tr>
                          <% }); %>
                        </tbody>
                      </table>
                    </div>
                  </div>

                  <div class="col-lg-4">
                    <div class="grand-total-wrap">
                      <h4>Cart totals</h4>
                      <div class="grand-total-content">
                        <ul>
                          <li>Subtotal <span>$ <%=subtotal%> </span></li>
                          <li>Total <span>$ <%=subtotal%></span></li>
                        </ul>
                      </div>

                      <div class="grand-btn">
                        <a href="checkout">Proceed to checkout</a>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>

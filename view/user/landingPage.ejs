      <div class="product-area pt-130  section-padding-3">
        <div class="container-fluid">
          <div class="section-title-2 mb-45 text-center">
            <h2>New arrival</h2>
          </div>
          <div class="product-tab-list nav mb-50">
            <!-- Generate category tabs dynamically -->
            <a class="active" href="#product-all" data-category="all" onclick="filterByCategory('all')"> All </a>
            <% categories.forEach((category) => { %>
              <a href="#product-<%= category._id %>" data-category="<%= category._id %>" onclick="filterByCategory('<%= category._id %>')">
                <%= category.name %>
              </a>
            <% }) %>
          </div>
          <div class="tab-content jump padding-60-row-col">
            <div id="product-1" class="tab-pane active">
              <div id="product-list" class="row">
                <!-- Section for New Arrivals -->
                <div class="section-title text-center mb-50">
                  <h2>New Arrivals</h2>
                </div>
                <% if (newArrivals.length > 0) { %>
                  <% newArrivals.forEach((product) => { %>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-12 product-card">
                      <div class="product-wrap mb-50" style="width: 333px;">
                        <div class="product-img default-overlay mb-25">
                          <a href="/product-guest-details/<%= product._id %>">
                            <img
                              class="default-img"
                              src="/uploads/<%= product.image_url && product.image_url[0] ? product.image_url[0] : '//assets/images/default-product.jpg' %>"
                              alt="<%= product.product_title %>"
                            />
                            <img
                              class="hover-img"
                              src="/uploads/<%= product.image_url && product.image_url[1] ? product.image_url[1] : '//assets/images/default-product-hover.jpg' %>"
                              alt="<%= product.product_title %>"
                            />
                          </a>
                        </div>
                        <div class="product-content-2 text-center">
                          <h3>
                            <a href="/product-guest-details/<%= product._id %>"><%= product.product_title %></a>
                          </h3>
                          <div class="product-price">
                            <span class="new-price">₹<%= product.price %></span>
                          </div>
                        </div>
                      </div>
                    </div>
                  <% }); %>
                <% } else { %>
                  <p>No new arrivals at the moment.</p>
                <% } %>
              
             
              

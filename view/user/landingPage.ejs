<style>
  /* General styles for the navigation menu */
  .main-menu ul li {
    display: inline-block;
    margin: 0 10px;
    position: relative;
  }
  
  .main-menu ul li a {
    color: #333; /* Default text color */
    text-decoration: none; /* Remove default underline */
    padding-bottom: 5px; /* Space for underline effect */
    transition: all 0.3s ease; /* Smooth transition */
  }
  
  /* Hover effect with a custom underline */
  .main-menu ul li a:hover {
    color: #000000; /* Text color on hover */
    border-bottom: 2px solid #000000; /* Underline color */
  }
  
  /* Optional: Add more spacing for the hover underline effect */
  .main-menu ul li a:hover::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 2px;
    background-color: #000000; /* Same underline color */
    transform: scaleX(1);
    transition: transform 0.3s ease;
  }
  
  /* Reset styles for no underline initially */
  .main-menu ul li a::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 2px;
    background-color: transparent; /* Invisible */
    transform: scaleX(0); /* No underline initially */
    transition: transform 0.3s ease, background-color 0.3s ease;
  }
  </style>
  


